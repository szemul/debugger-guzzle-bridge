{
    "name": "szemul/debugger-guzzle-bridge",
    "description": "Guzzle middleware to allow the `szemul/debugger` package to debug HTTP requests sent via guzzle",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^6.5.0|^7.0.0",
        "szemul/debugger": "^1.0.1|^2.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0.0",
        "jetbrains/phpstorm-attributes": "^1.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^1.10.25",
        "phpunit/phpunit": "^9.5.5"
    },
    "autoload": {
        "psr-4": {
            "Szemul\\DebuggerGuzzleBridge\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Szemul\\DebuggerGuzzleBridge\\Test\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
